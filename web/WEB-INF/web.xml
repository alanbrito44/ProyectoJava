<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginServelet</servlet-name>
        <servlet-class>com.controlador.LoginServelet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MarcaServlet</servlet-name>
        <servlet-class>com.controlador.MarcaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>blogControl</servlet-name>
        <servlet-class>com.controlador.blogControl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AutomovilServlet</servlet-name>
        <servlet-class>com.controlador.AutomovilServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClientesServlet</servlet-name>
        <servlet-class>com.controlador.ClientesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>prueba</servlet-name>
        <servlet-class>com.controlador.prueba</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CatAutoServlet</servlet-name>
        <servlet-class>com.controlador.CatAutoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsuarioServlet</servlet-name>
        <servlet-class>com.controlador.UsuarioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VentasController</servlet-name>
        <servlet-class>com.controlador.VentasController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServelet</servlet-name>
        <url-pattern>/LoginServelet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MarcaServlet</servlet-name>
        <url-pattern>/MarcaServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>blogControl</servlet-name>
        <url-pattern>/blogControl</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AutomovilServlet</servlet-name>
        <url-pattern>/AutomovilServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClientesServlet</servlet-name>
        <url-pattern>/ClientesServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>prueba</servlet-name>
        <url-pattern>/prueba</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CatAutoServlet</servlet-name>
        <url-pattern>/CatAutoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuarioServlet</servlet-name>
        <url-pattern>/UsuarioServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VentasController</servlet-name>
        <url-pattern>/VentasController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
